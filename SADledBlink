// SADledBlink
// Scott A. Dougherty
// 2017 09 06

// name variables for the LED pin and delay
int ledPin = 13; 
int delayPeriodA = 500;
int delayPeriodB = 300;

// define setup by configuring pin 13 as an output
void setup()
{
pinMode(ledPin, OUTPUT);
}

void loop()
{
digitalWrite(ledPin, HIGH);
delay(delayPeriodA);
digitalWrite(ledPin, LOW);
delay(delayPeriodB);
}
