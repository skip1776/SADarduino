//  RepeatingMyself

//  Scott A. Dougherty, papercutter@comcast.net
//  2017 09 06

//  Use Ctrl+Shift+M to display the serial monitor

void setup() {
Serial.begin(9600);
}

void loop() {
String comment = "Arduino: I feel like I keep repeating myself.";
delay(2000);
Serial.println(comment);
String commentB = "User: What?";
delay(2500);
Serial.println(commentB);
}
